[sub_resource type="Shader" id=2]
code = "shader_type canvas_item;

//https://nathanhoad.net/2021-03-water-and-lava-shaders-in-godot/



varying vec2 vert;
uniform float speed = 0.1;
uniform float waveLength = 0.2f;
uniform float blend = 0.5f;
uniform vec4 color : hint_color;

void vertex() {
    vert = VERTEX;
}

void fragment() {
    vec4 input = texture(TEXTURE, UV);
	
    if (input.a == 0.0) {
        COLOR = input;
		}
	else
	{
		vec2 tiled_uvs = UV *0.5;
        vec2 displacement = vec2(
            cos( speed* TIME * 5.0 + tiled_uvs.x + tiled_uvs.y) * waveLength,
            cos(speed* TIME * 5.0 + tiled_uvs.x + tiled_uvs.y) * waveLength
        );
		// Below the surface
		
		vec4 waterTex = color;//texture(TEXTURE, UV + displacement);
		
		
		
		vec4 groundTex = texture(SCREEN_TEXTURE, SCREEN_UV + displacement*0.05);
        vec4 refraction = mix(waterTex, groundTex, blend);
		
		bool mask = input.r == 1.0f;
		refraction.a = 1.0 * float(!mask);
		COLOR = refraction;
		
	}
    
}"

